/** 
 * Multiplexor:
 * out = a if sel == 0
 *       b otherwise
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    Not(in=sel, out=not_sel);
    And(a=a, b=sel, out=sel_a);
    And(a=b, b=sel, out=sel_b);
    Or(a=sel_a, b=sel_b, out=out);
}